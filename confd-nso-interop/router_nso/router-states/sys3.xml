<config xmlns="http://tail-f.com/ns/config/1.0">
  <sys xmlns="http://example.com/router">
    <interfaces>
      <interface>
        <name>eth11</name>
        <unit>
          <name>1</name>
          <enabled>true</enabled>
          <family>
            <inet>
              <address>
                <name>192.168.1.3</name>
                <prefix-length>16</prefix-length>
              </address>
              <address>
                <name>192.168.1.4</name>
                <prefix-length>16</prefix-length>
              </address>
            </inet>
          </family>
        </unit>
        <unit>
          <name>2</name>
          <enabled>true</enabled>
          <description>My Vlan</description>
          <vlan-id>18</vlan-id>
        </unit>
      </interface>
      <interface>
        <name>eth2</name>
        <description>some description</description>
        <enabled/>
        <speed>thousand</speed>
        <duplex>full</duplex>
        <mtu>1500</mtu>
        <mac>11:22:33:44:55:00</mac>
        <unit>
          <name>2</name>
          <enabled>true</enabled>
          <description>myarp2</description>
          <arp>no-gratuitous-arp-reply</arp>
          <family>
            <inet>
              <address>
                <name>127.0.0.1</name>
                <prefix-length>24</prefix-length>
                <broadcast>127.0.0.1</broadcast>
              </address>
            </inet>
          </family>
        </unit>
      </interface>
      <serial>
        <name>ppp1</name>
        <ppp>
          <accounting>acme</accounting>
        </ppp>
        <authentication>
          <method>pap</method>
        </authentication>
        <authorization>admin</authorization>
      </serial>
      <serial>
        <name>s0</name>
        <authentication>
          <method>pap</method>
          <list-name>mylistname</list-name>
          <one-time/>
          <optional/>
        </authentication>
      </serial>
    </interfaces>
    <routes>
      <inet>
        <route>
          <name>10.11.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 1</description>
          <next-hop>
            <name>192.168.10.1</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>10.21.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 2</description>
          <next-hop>
            <name>192.168.10.2</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>10.31.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 3</description>
          <next-hop>
            <name>192.168.10.3</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>10.41.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 4</description>
          <next-hop>
            <name>192.168.10.4</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>10.51.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 5</description>
          <next-hop>
            <name>192.168.10.5</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>10.61.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 6</description>
          <next-hop>
            <name>192.168.10.6</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>10.71.0.0</name>
          <prefix-length>16</prefix-length>
          <description>Route 7</description>
          <next-hop>
            <name>192.168.10.7</name>
            <metric>100</metric>
          </next-hop>
        </route>
        <route>
          <name>192.168.0.0</name>
          <prefix-length>24</prefix-length>
          <description>some route</description>
          <enabled>true</enabled>
          <next-hop>
            <name>192.168.99.1</name>
            <metric>1</metric>
          </next-hop>
        </route>
      </inet>
    </routes>
    <syslog>
      <server>
        <name>10.31.4.5</name>
        <enabled>true</enabled>
        <selector>
          <name>8</name>
          <facility>auth</facility>
          <facility>authpriv</facility>
          <facility>local0</facility>
        </selector>
        <administrator>Joe Smith</administrator>
      </server>
      <server>
        <name>192.168.0.23</name>
        <enabled>true</enabled>
        <selector>
          <name>1</name>
          <negate>true</negate>
          <comparison>same-or-higher</comparison>
          <level>debug</level>
          <facility>ftp</facility>
        </selector>
      </server>
      <server>
        <name>some-server</name>
      </server>
    </syslog>
    <ntp>
      <server>
        <name>10.21.3.4</name>
        <key>2</key>
      </server>
      <local-clock>
        <enabled>true</enabled>
        <stratum>15</stratum>
        <status>
          <jitter>12</jitter>
        </status>
      </local-clock>
      <key>
        <name>1</name>
        <value>somevalue</value>
        <trusted>true</trusted>
      </key>
      <key>
        <name>2</name>
      </key>
      <controlkey>2</controlkey>
    </ntp>
    <dns>
      <search>
        <name>1</name>
        <domain>tail-f.com</domain>
      </search>
      <server>
        <address>1.1.2.1</address>
      </server>
      <server>
        <address>10.21.3.4</address>
      </server>
      <options>
        <ndots>1</ndots>
        <timeout>100</timeout>
        <attempts>1</attempts>
      </options>
    </dns>
  </sys>
</config>

